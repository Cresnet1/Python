sum = 0
for numbers in range(1,21):
  if numbers % 2 == 0:
    sum += numbers
print(sum)




adj = ["red", "big", "tasty"]
fruits = ["apple", "banana", "cherry"]

for x in adj:
  for y in fruits:
    print(x, y)

for x in range(1,4):
  for i in range(1,4):
    print(x,i)

